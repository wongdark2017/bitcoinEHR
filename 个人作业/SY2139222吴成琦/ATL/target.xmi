<?xml version="1.0" encoding="ASCII"?>
<occi:Configuration xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:occi="http://schemas.ogf.org/occi">
  <use href="erocci.xmi#/"/>
  <resources id="http">
    <kind href="erocci.xmi#//@kinds[term='listener']"/>
    <attributes name="port" value="80"/>
    <links id="link-1" target="//@resources.1">
      <kind href="erocci.xmi#//@kinds[term='frontendlink']"/>
    </links>
  </resources>
  <resources id="core">
    <kind href="erocci.xmi#//@kinds[term='core']"/>
    <links id="link-2" target="//@resources.2">
      <kind href="erocci.xmi#//@kinds[term='backendlink']"/>
      <attributes name="path" value="/"/>
    </links>
  </resources>
  <resources id="mnesia">
    <kind href="erocci.xmi#//@kinds[term='backend']"/>
  </resources>
</occi:Configuration>